{% extends 'base.html.twig' %}

{% block title %}gallery{% endblock %}

{% block body %}

<div class="mainContainer">
    <h1>GALLERY</h1>
    {% for scene in scenes %}
    <div class="cardGallery">
        <img style="max-width: 400px" src="{{ vich_uploader_asset(scene, 'imageFile') }}" alt="{{ scene.user.name }}">
    </div>
    <button><a href="{{ path('newScene', {'id': scene.id})}}"> Open scene with parameters</a> </button>
      


    {% endfor %}


</div>

{% endblock %}
